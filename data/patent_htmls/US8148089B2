              <tr class="description-tr">
                <td class="description-td">Completely Confirmed Hits</td>
              </tr>
              <tr class="description-tr">
                <td class="description-td">
                  <chemistry id="CHEM-US-00008" num="00008">
                    <div class="patent-image"> <a href="//patentimages.storage.googleapis.com/US8148089B2/US08148089-20120403-C00008.png"> <img id="EMI-C00008" he="39.96mm" wi="118.45mm" file="US08148089-20120403-C00008.TIF" alt="Figure US08148089-20120403-C00008" img-content="table" img-format="tif" orientation="portrait" inline="no" src="//patentimages.storage.googleapis.com/US8148089B2/US08148089-20120403-C00008.png" class="patent-full-image" width="473" height="159"> </a> </div>
