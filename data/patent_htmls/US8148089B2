              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td">SYN APEG1</td>
                <td class="description-td">2.23</td>
              </tr>
              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td">SYN SNK/PLK2</td>
                <td class="description-td">3.57</td>
              </tr>
              <tr class="description-tr">
