            <colspec colname="3" colwidth="77pt" align="center"> </colspec>
            <colspec colname="4" colwidth="63pt" align="center"> </colspec>
            <tbody valign="top">
              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td"> </td>
                <td class="description-td"> </td>
                <td class="description-td">Catalytic Efficiency</td>
              </tr>
              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td"> </td>
                <td class="description-td">Catalytic Efficiency</td>
                <td class="description-td">against (−)-cocaine</td>
              </tr>
