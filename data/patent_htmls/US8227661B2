                <td class="description-td">F</td>
                <td class="description-td">0</td>
                <td class="description-td">17</td>
                <td class="description-td">F</td>
                <td class="description-td">26</td>
                <td class="description-td">23</td>
                <td class="description-td">N/A</td>
              </tr>
              <tr class="description-tr">
                <td namest="1" nameend="7" align="center" rowsep="1" class="description-td" colspan="7"> </td>
