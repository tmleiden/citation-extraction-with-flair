            <colspec colname="3" colwidth="49pt" align="center"> </colspec>
            <colspec colname="4" colwidth="21pt" align="center"> </colspec>
            <colspec colname="5" colwidth="56pt" align="center"> </colspec>
            <tbody valign="top">
              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td">Observed</td>
                <td class="description-td">25</td>
                <td class="description-td">46</td>
                <td class="description-td">29</td>
                <td class="description-td">100</td>
              </tr>
              <tr class="description-tr">
                <td class="description-td"> </td>
                <td class="description-td">Expected</td>
                <td class="description-td">25</td>
                <td class="description-td">50</td>
                <td class="description-td">25</td>
                <td class="description-td">100</td>
